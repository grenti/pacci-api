{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "modules": true
    },
    "ecmaVersion": 8,
    "sourceType": "module"
  },
  "globals": {
    "node": true,
    "commonjs": true
  },
  "extends": "standard",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/jsx-quotes": 0,
    "jsx-quotes": 1
  }
}
